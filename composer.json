{
    "name": "illuminated/console-mutex",
    "description": "Mutex for Laravel Console Commands.",
    "keywords": ["laravel", "console", "command", "mutex", "mutex-lock", "semaphore", "locker", "no-overlapping"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dmitry-ivanov/laravel-console-mutex/issues",
        "source": "https://github.com/dmitry-ivanov/laravel-console-mutex"
    },
    "authors": [{
        "name": "Dmitry Ivanov",
        "email": "dmitry.g.ivanov@gmail.com"
    }],
    "require": {
        "php": "^8.2",
        "arvenil/ninja-mutex": "dev-master#82cbb2c",
        "illuminate/console": "^11.21.0",
        "illuminate/support": "^11.21.0"
    },
    "require-dev": {
        "ext-redis": "*",
        "ext-pdo_mysql": "*",
        "phpunit/phpunit": "^11.3.1",
        "mockery/mockery": "^1.6.12",
        "orchestra/testbench": "^9.4.0",
        "illuminated/testing-tools": "^10.0",
        "predis/predis": "^2.2.2"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Illuminated\\Console\\Tests\\": "tests/",
            "Illuminated\\Console\\Tests\\App\\": "tests/fixture/app/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
